{
        "name": "cra-template-truemark",
        "version": "1.0.0",
        "description": "CRA Template with setups including, eslint, prettier, husky and reviewdog",
        "author": "Truemark Technology",
        "dependencies": {
                "@fortawesome/fontawesome-svg-core": "^6.4.2",
                "@fortawesome/free-brands-svg-icons": "^6.4.2",
                "@fortawesome/free-solid-svg-icons": "^6.4.2",
                "@fortawesome/react-fontawesome": "^0.2.0",
                "@testing-library/react": "^10.4.0",
                "@testing-library/user-event": "^12.0.0",
                "@types/testing-library__jest-dom": "^6.0.0",
                "axios": "^1.5.0",
                "bootstrap": "^5.3.1",
                "i18next": "^23.4.6",
                "i18next-browser-languagedetector": "^7.1.0",
                "i18next-http-backend": "^2.2.1",
                "jest-junit": "^16.0.0",
                "react": "^18.2.0",
                "react-dom": "^18.2.0",
                "react-i18next": "^13.2.1",
                "react-router-dom": "^6.15.0",
                "react-scripts": "^5.0.1",
                "start-server-and-test": "^2.0.0"
        },
        "scripts": {
                "start": "react-scripts start",
                "build": "react-scripts build",
                "test": "react-scripts test",
                "eject": "react-scripts eject",
                "prepare": "husky install",
                "format": "prettier --write \"src/**/*.js\"",
                "lint:style": "stylelint \"**/*.scss\"",
                "lint:style:fix": "stylelint --fix \"**/*.scss\"",
                "lint": "eslint .",
                "lint:fix": "eslint --fix --ext .js,.jsx .",
                "pre-commit": "lint-staged",
                "test:e2e": "cypress open",
                "test:headless:e2e": "cypress run",
                "test:record": "cypress run --record",
                "test:headless:browser:e2e": "cypress run -b chrome --headless",
                "start:ci": "start-server-and-test start http://localhost:3000/ test:headless:browser:e2e",
                "jest:ci": "npm run test -- --testResultsProcessor=\"jest-junit\" --watchAll=false --ci"
        },
        "devDependencies": {
                "@babel/core": "^7.22.11",
                "@babel/eslint-parser": "^7.22.11",
                "@babel/preset-react": "^7.22.5",
                "@testing-library/jest-dom": "^6.1.2",
                "cypress": "^13.0.0",
                "eslint": "^8.48.0",
                "eslint-config-prettier": "^9.0.0",
                "eslint-plugin-jest-dom": "^5.1.0",
                "eslint-plugin-jsx-a11y": "^6.7.1",
                "eslint-plugin-prettier": "^3.4.1",
                "eslint-plugin-react": "^7.33.2",
                "eslint-plugin-unused-imports": "^2.0.0",
                "husky": "^8.0.3",
                "lint-staged": "^14.0.1",
                "prettier": "^3.0.3",
                "pretty-quick": "^3.1.3",
                "sass": "^1.66.1",
                "stylelint": "^14.16.1",
                "stylelint-config-sass-guidelines": "^9.0.1",
                "stylelint-order": "^6.0.3",
                "stylelint-scss": "^5.1.0"
        },
        "lint-staged": {
                "**/*.scss": [
                        "stylelint --fix",
                        "stylelint \"**/*.scss\""
                ],
                "**/*.{js,jsx}": [
                        "eslint --cache --fix",
                        "prettier --write",
                        "eslint  --ext .js,.jsx ."
                ]
        },
        "browserslist": {
                "production": [
                        ">0.2%",
                        "not dead",
                        "not op_mini all"
                ],
                "development": [
                        "last 1 chrome version",
                        "last 1 firefox version",
                        "last 1 safari version"
                ]
        },
        "engines": {
                "node": "^18.14.0"
        }
}